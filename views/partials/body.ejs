<table>
    <tr>
        <th>Event</th>
        <th>Description</th>
        <th>Date</th>
    </tr>
    <% events.forEach(function(event) { %>
        <tr>
            <td><%= event.name %></td>
            <td><%= event.description %></td>
            <td><%= event.date %></td>
        </tr>
    <% }); %>
  </table>